{
  "$schema": "https://developers.reddit.com/schema/config-file.v1.json",
  "name": "myvoxel234",
  "post": {
    "dir": "dist/client",
    "entrypoints": {
      "default": {
        "entry": "index.html"
      }
    }
  },
  "server": {
    "dir": "dist/server",
    "entry": "index.cjs"
  },
  "media": {},
  "menu": {
    "items": [
      {
        "label": "Create a new post",
        "description": "myvoxel234",
        "location": "subreddit",
        "forUserType": "moderator",
        "endpoint": "/internal/menu/post-create"
      },
      {
        "label": "Create Voxel World",
        "description": "Initialize world config (terrain + seed)",
        "location": "subreddit",
        "forUserType": "moderator",
        "endpoint": "/internal/menu/start-world-create"
      }
      
    ]
  },
  "triggers": {
    "onAppInstall": "/internal/on-app-install"
  },
  "forms": {
    "worldConfigForm": "/internal/form/world-config-submit",
    "worldConfigCreateForm": "/internal/form/world-config-create"
  },
  "permissions": {
    "redis": true,
    "media": true
  },
  "dev": {
    "subreddit": "myvoxel234_dev"
  }
}